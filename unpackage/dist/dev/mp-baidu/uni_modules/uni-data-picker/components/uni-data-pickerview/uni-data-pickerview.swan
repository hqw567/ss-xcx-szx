<view class="uni-data-pickerview"><scroll-view class="selected-area" scroll-x="true" scroll-y="false" show-scrollbar="{{false}}"><view class="selected-list"><block s-for="selected" s-for-item="item" s-for-index="index"><block s-if="{{item.text}}"><view data-event-opts="{{[['tap',[['handleSelect',[index]]]]]}}" class="{{['selected-item',(index==selectedIndex)?'selected-item-active':'',(ellipsis)?'selected-item-text-overflow':'']}}" bindtap="__e"><text>{{item.text}}</text></view></block></block></view></scroll-view><view class="tab-c"><block s-for="dataList trackBy i" s-for-item="child" s-for-index="i"><block s-if="{{i==selectedIndex}}"><scroll-view class="list" scroll-y="{{true}}"><block s-for="child" s-for-item="item" s-for-index="j"><view data-event-opts="{{[['tap',[['handleNodeClick',['$0',i,j],[[['dataList','',i],['','',j]]]]]]]}}" class="{{['item',(!!item.disable)?'is-disabled':'']}}" bindtap="__e"><text class="item-text item-text-overflow">{{item[map.text]}}</text><block s-if="{{selected.length>i&&item[map.value]==selected[i].value}}"><view class="check"></view></block></view></block></scroll-view></block></block><block s-if="{{loading}}"><view class="loading-cover"><uni-load-more class="load-more" vue-id="d6eb1d26-1" contentText="{{loadMore}}" status="loading"></uni-load-more></view></block><block s-if="{{errorMessage}}"><view class="error-message"><text class="error-text">{{errorMessage}}</text></view></block></view></view>