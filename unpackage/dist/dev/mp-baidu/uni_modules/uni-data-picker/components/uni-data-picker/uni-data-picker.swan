<view class="uni-data-tree"><view data-event-opts="{{[['tap',[['handleInput',['$event']]]]]}}" class="uni-data-tree-input" bindtap="__e"><block s-if="{{$slots.default}}"><slot var-options="options" var-data="inputSelected" var-error="errorMessage"></slot></block><block s-else><view class="{{['input-value',(border)?'input-value-border':'']}}"><block s-if="{{errorMessage}}"><text class="selected-area error-text">{{errorMessage}}</text></block><block s-else><block s-if="{{loading&&!isOpened}}"><view class="selected-area"><uni-load-more class="load-more" vue-id="c91b1312-1" contentText="{{loadMore}}" status="loading"></uni-load-more></view></block><block s-else><block s-if="{{inputSelected.length}}"><scroll-view class="selected-area" scroll-x="true"><view class="selected-list"><block s-for="inputSelected trackBy index" s-for-item="item" s-for-index="index"><view class="selected-item"><text class="text-color">{{item.text}}</text><block s-if="{{index<inputSelected.length-1}}"><text class="input-split-line">{{split}}</text></block></view></block></view></scroll-view></block><block s-else><text class="selected-area placeholder">{{placeholder}}</text></block></block></block><block s-if="{{clearIcon&&!readonly&&inputSelected.length}}"><view data-event-opts="{{[['tap',[['clear',['$event']]]]]}}" class="icon-clear" catchtap="__e"><uni-icons vue-id="c91b1312-2" type="clear" color="#c0c4cc" size="24"></uni-icons></view></block><block s-if="{{(!clearIcon||!inputSelected.length)&&!readonly}}"><view class="arrow-area"><view class="input-arrow"></view></view></block></view></block></view><block s-if="{{isOpened}}"><view data-event-opts="{{[['tap',[['handleClose',['$event']]]]]}}" class="uni-data-tree-cover" bindtap="__e"></view></block><block s-if="{{isOpened}}"><view class="uni-data-tree-dialog"><view class="uni-popper__arrow"></view><view class="dialog-caption"><view class="title-area"><text class="dialog-title">{{popupTitle}}</text></view><view data-event-opts="{{[['tap',[['handleClose',['$event']]]]]}}" class="dialog-close" bindtap="__e"><view class="dialog-close-plus" data-id="close"></view><view class="dialog-close-plus dialog-close-rotate" data-id="close"></view></view></view><data-picker-view class="picker-view vue-ref" vue-id="c91b1312-3" localdata="{{localdata}}" preload="{{preload}}" collection="{{collection}}" field="{{field}}" orderby="{{orderby}}" where="{{where}}" step-searh="{{stepSearh}}" self-field="{{selfField}}" parent-field="{{parentField}}" managed-mode="{{true}}" map="{{map}}" ellipsis="{{ellipsis}}" data-ref="pickerView" value="{{dataValue}}" data-event-opts="{{[['^change',[['onchange']]],['^datachange',[['ondatachange']]],['^nodeclick',[['onnodeclick']]],['^input',[['__set_model',['','dataValue','$event',[]]]]]]}}" bind:change="__e" bind:datachange="__e" bind:nodeclick="__e" bind:input="__e"></data-picker-view></view></block></view>