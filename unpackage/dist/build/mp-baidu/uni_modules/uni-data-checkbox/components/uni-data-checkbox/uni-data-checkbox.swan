<view class="uni-data-checklist" style="{{'margin-top:'+(isTop+'px')+';'}}"><block s-if="{{!isLocal}}"><view class="uni-data-loading"><block s-if="{{!mixinDatacomErrorMessage}}"><uni-load-more vue-id="15e8047c-1" status="loading" iconType="snow" iconSize="{{18}}" content-text="{{contentText}}"></uni-load-more></block><block s-else><text>{{mixinDatacomErrorMessage}}</text></block></view></block><block s-else><block s-if="{{multiple}}"><checkbox-group data-event-opts="{{[['change',[['chagne',['$event']]]]]}}" class="{{['checklist-group',(mode==='list'||wrap)?'is-list':'']}}" bindchange="__e"><block s-for="dataList trackBy index" s-for-item="item" s-for-index="index"><label class="{{['checklist-box','is--'+mode,item.selected?'is-checked':'',disabled||!!item.disabled?'is-disable':'',index!==0&&mode==='list'?'is-list-border':'']}}" style="{{(item.styleBackgroud)}}"><checkbox class="hidden" hidden="{{true}}" disabled="{{disabled||!!item.disabled}}" value="{{item[map.value]+''}}" checked="{{item.selected}}"></checkbox><block s-if="{{mode!=='tag'&&mode!=='list'||mode==='list'&&icon==='left'}}"><view class="checkbox__inner" style="{{(item.styleIcon)}}"><view class="checkbox__inner-icon"></view></view></block><view class="{{['checklist-content',(mode==='list'&&icon==='left')?'list-content':'']}}"><text class="checklist-text" style="{{(item.styleIconText)}}">{{item[map.text]}}</text><block s-if="{{mode==='list'&&icon==='right'}}"><view class="checkobx__list" style="{{(item.styleBackgroud)}}"></view></block></view></label></block></checkbox-group></block><block s-else><radio-group data-event-opts="{{[['change',[['chagne',['$event']]]]]}}" class="{{['checklist-group',(mode==='list')?'is-list':'',(wrap)?'is-wrap':'']}}" bindchange="__e"><block s-for="dataList trackBy index" s-for-item="item" s-for-index="index"><label class="{{['checklist-box','is--'+mode,item.selected?'is-checked':'',disabled||!!item.disabled?'is-disable':'',index!==0&&mode==='list'?'is-list-border':'']}}" style="{{(item.styleBackgroud)}}"><radio class="hidden" hidden="{{true}}" disabled="{{disabled||item.disabled}}" value="{{item[map.value]+''}}" checked="{{item.selected}}"></radio><block s-if="{{mode!=='tag'&&mode!=='list'||mode==='list'&&icon==='left'}}"><view class="radio__inner" style="{{(item.styleBackgroud)}}"><view class="radio__inner-icon" style="{{(item.styleIcon)}}"></view></view></block><view class="{{['checklist-content',(mode==='list'&&icon==='left')?'list-content':'']}}"><text class="checklist-text" style="{{(item.styleIconText)}}">{{item[map.text]}}</text><block s-if="{{mode==='list'&&icon==='right'}}"><view class="checkobx__list" style="{{(item.styleRightIcon)}}"></view></block></view></label></block></radio-group></block></block></view>