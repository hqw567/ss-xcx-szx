button::after{border:none}button{position:relative;display:block;margin-left:auto;margin-right:auto;padding-left:0;padding-right:0;box-sizing:border-box;text-align:center;text-decoration:none;line-height:1.35;-webkit-tap-highlight-color:transparent;overflow:hidden;color:#707070;background-color:initial;width:100%;height:100%}.school-item{display:flex;border-radius:5px;background-color:#fff;padding:13px 15px;margin-top:8px}.school-item .item-mid{width:75%;margin-left:8px;height:80px;display:flex;flex-direction:column;justify-content:space-between}.school-item .img{width:80px;height:80px;flex-shrink:0}.school-item .title{font-weight:700;font-size:16px;color:#333;overflow:hidden;text-overflow:ellipsis;white-space:nowrap}.school-item .tags{font-size:12px;color:#78c8ff;display:flex;flex-wrap:wrap;height:20px;overflow:hidden}.school-item .tags > .tag-item{border:1px solid #78c8ff;border-radius:10px;padding:0 8px;height:18px;margin-bottom:5px;white-space:nowrap;margin-right:5px}.school-item .info{font-size:13px;color:#838383}